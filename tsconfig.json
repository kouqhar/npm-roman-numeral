{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    // "declaration": true,
    "outDir": "./dist",
    "rootDir": "./"
  },
  "include": ["**/*.ts", "jest.config.js"],
  "exclude": ["node_modules", "dist"]
}